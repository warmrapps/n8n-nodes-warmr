{
  "name": "n8n-nodes-warmr",
  "version": "2.0.16",
  "description": "n8n community node for Warmr integration",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "author": "Warmr <support@warmr.app>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/warmrapps/n8n-nodes-warmr"
  },
  "keywords": [
    "n8n",
    "node",
    "warmr",
    "integration",
    "community"
  ],
  "devDependencies": {
    "@types/node": "^24.0.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0",
    "node-fetch": "^3.3.2"
  },
  "files": [
    "build"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "build/credentials/WarmrApi.credentials.js"
    ],
    "nodes": [
      "build/nodes/Contacts.node.js"
    ]
  },
  "scripts": {
    "build": "pnpm bump && tsc",
    "bump": "npm version patch --no-git-tag-version",
    "buildAndPublish": "pnpm run build && npm publish --access public"
  }
}
